{
  "name": "citty-test-utils-playground",
  "version": "1.0.0",
  "description": "Playground for testing citty-test-utils functionality",
  "type": "module",
  "main": "src/cli.mjs",
  "bin": {
    "playground": "src/cli.mjs"
  },
  "scripts": {
    "start": "node src/cli.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:fast": "vitest run --reporter=basic --no-coverage",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:comprehensive": "node test-comprehensive.mjs",
    "test:fluent": "vitest run test/integration/fluent-assertions.test.mjs",
    "test:utils": "vitest run test/integration/test-utils.test.mjs",
    "test:scenarios": "vitest run test/integration/advanced-scenarios.test.mjs",
    "test:analysis": "vitest run test/integration/analysis-tools.test.mjs",
    "test:snapshots": "vitest run test/integration/snapshot-testing.test.mjs",
    "demo": "node scenarios-examples.mjs",
    "demo:snapshots": "node snapshot-examples.mjs",
    "demo:analysis": "node analysis-examples.mjs"
  },
  "keywords": [
    "citty",
    "testing",
    "playground",
    "cli",
    "examples"
  ],
  "author": "GitVan Team",
  "license": "MIT",
  "dependencies": {
    "citty": "^0.1.6",
    "citty-test-utils": "^0.5.1"
  },
  "devDependencies": {
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

